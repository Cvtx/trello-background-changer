{
  "name": "Trello Background Changer",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "",
  "homepage_url": "https://github.com/Cvtx/trello-background-changer",
  "permissions": [
    "https://trello.com/b/*"
  ],
  "content_scripts": [
